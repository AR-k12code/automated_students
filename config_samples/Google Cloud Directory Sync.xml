  <logging>
    <defaultLogLevel>INFO</defaultLogLevel>
    <format>[%d{ISO8601}%d{Z}] [%t] [%p] [%C{3}] %m%n</format>
    <logFile>C:\scripts\gcds\gentrysms.log</logFile>
    <maximumLogBytes>1073741824</maximumLogBytes>
    <logFileCount>1</logFileCount>
  </logging>
  <notification>
    <smtpRelay>smtp-relay.gmail.com</smtpRelay>
    <useSmtpTLS>true</useSmtpTLS>
    <fromAddress>noreply@facultydomain.com</fromAddress>
    <toAddress>technology@facultydomain.com</toAddress>
    <useAttachment>true</useAttachment>
    <ignoreRetrievalInfo>false</ignoreRetrievalInfo>
    <ignoreRetrievalWarnings>false</ignoreRetrievalWarnings>
    <ignoreRetrievalErrors>false</ignoreRetrievalErrors>
  </notification>
  <eventLimits>
    <deleteLimitPCT_CALENDAR_RESOURCE>5</deleteLimitPCT_CALENDAR_RESOURCE>
    <deleteLimitCount_GROUP>1000</deleteLimitCount_GROUP>
    <modifyLimitPCT_GROUP>50</modifyLimitPCT_GROUP>
    <deleteLimitPCT_IDENTITY_MAPPED_GROUP>5</deleteLimitPCT_IDENTITY_MAPPED_GROUP>
    <deleteLimitPCT_ORG_UNIT>5</deleteLimitPCT_ORG_UNIT>
    <deleteLimitPCT_SHARED_CONTACT>5</deleteLimitPCT_SHARED_CONTACT>
    <deleteLimitCount_USER>50</deleteLimitCount_USER>
    <suspendLimitCount_USER>500</suspendLimitCount_USER>
    <modifyMinimumMemberGroupLimit>100</modifyMinimumMemberGroupLimit>
  </eventLimits>
  <!--cache lifetime specified in seconds, default value is 8 days -->
  <maxCacheLifetime>691200</maxCacheLifetime>
  <!--specifies whether to calculate cache lifetime dynamically or not. If set to false, maxCacheLifetime value is used as cache lifetime. If set to true, then cache lifetime is calculated dynamically -->
  <useDynamicMaxCacheLifetime>false</useDynamicMaxCacheLifetime>
